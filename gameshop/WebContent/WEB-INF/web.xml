<?xml version="1.0" encoding="UTF-8"?>
<web-app 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" 
id="WebApp_ID" 
version="4.0">

<jsp-config>
 <jsp-property-group>
   <url-pattern>*.jsp</url-pattern>
   <include-prelude>/WEB-INF/tagdirs.jsp</include-prelude>
 </jsp-property-group>
</jsp-config>  

<servlet>
	<servlet-name>Accueil</servlet-name>
	<servlet-class>servlets.Accueil</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>Accueil</servlet-name>
	<url-pattern>/Accueil</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>Connexion</servlet-name>
	<servlet-class>servlets.Connexion</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>Connexion</servlet-name>
	<url-pattern>/Connexion</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>Inscription</servlet-name>
	<servlet-class>servlets.Inscription</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>Inscription</servlet-name>
	<url-pattern>/Inscription</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>Panier</servlet-name>
	<servlet-class>servlets.Panier</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>Panier</servlet-name>
	<url-pattern>/Panier</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>Produit</servlet-name>
	<servlet-class>servlets.Produit</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>Produit</servlet-name>
	<url-pattern>/Produit</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>About</servlet-name>
	<servlet-class>servlets.About</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>About</servlet-name>
	<url-pattern>/About</url-pattern>
</servlet-mapping>

</web-app>